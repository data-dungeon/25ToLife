// test scoping of enums

{
   enum { A = 3, B, C };
   print A;  // result: 3
   print B;  // result: 4
   print C;  // result: 5
   {
      print A;  // result: 3
      print B;  // result: 4
      print C;  // result: 5
   }
}

{
   enum { B = 7, A, C };
   print A;  // result: 8
   print B;  // result: 7
   print C;  // result: 9
}
