switch( 1 )
{
case 1:
   switch( 3 )
   {
      case 1: print "bad 1"; break;
      case 2: print "bad 2"; break;
      case 3: print "good inside 3"; break; // result: good inside 3
   }
   break;
case 2:
   print "bad outside 2"; break;
case 3:
   print "bad outside 3"; break;
default:
   print "bad outside default"; break;
}

// result: out 1, in 1
// result: out 2, in 2
// result: out 3, in 3
