// result: out def, in 5
int v[ 16 ] = ( 11, 22, 33, 44, 55, 17, 27, 37, 47, 57, 77, 71, 72, 73, 74, 75 );
int i;
for ( i = 0 ; i < sizeof( v ) ; i++ )
{
   switch( v[i] / 10 )
   {
      case 1:
	switch( v[i] % 10 )
	{
	   case 1:  print "out 1, in 1";   break;
	   case 2:  print "out 1, in 2";   break;
	   case 3:  print "out 1, in 3";   break;
	   case 4:  print "out 1, in 4";   break;
