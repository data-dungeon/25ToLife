<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.10"
	Name="objAttrib"
	SccProjectName="&quot;//depot/Avalanche/Tools/Plugins/Maya/objAttrib&quot;"
	SccAuxPath=""
	SccLocalPath="."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="$(ConfigurationName)"
			IntermediateDirectory="$(ConfigurationName)"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="2"
				InlineFunctionExpansion="1"
				AdditionalIncludeDirectories="$(MAYAPATH)\include,$(TTLLIB)\Tools,$(TTLLIB)\GameLib"
				PreprocessorDefinitions="WIN32;NDEBUG;_WINDOWS;NT_PLUGIN;WIN32_TOOLS_RELEASE;$(AVUSER)"
				StringPooling="TRUE"
				RuntimeLibrary="2"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="3"
				PrecompiledHeaderThrough="pluginMainPCH.h"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="0"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Copying $(TargetFileName) to Maya Plugins Directory"
				CommandLine="copy &quot;$(TargetPath)&quot; &quot;$(MAYAPATH)\bin\plug-ins\$(TargetFileName)&quot;
"
				Outputs="$(MAYAPATH)\bin\plug-ins\$(TargetFileName)"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/export:initializePlugin /export:uninitializePlugin "
				AdditionalDependencies="shlwapi.lib comctl32.lib openmayaui.lib foundation.lib openmaya.lib"
				OutputFile="$(OutDir)/$(ProjectName).mll"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(MAYAPATH)\lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG;$(AVUSER)"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\Maya_Release/objAttrib.tlb"
				HeaderFileName=""/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG;$(AVUSER)"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCXMLDataGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
			<Tool
				Name="VCManagedWrapperGeneratorTool"/>
			<Tool
				Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
		</Configuration>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory="$(ConfigurationName)"
			IntermediateDirectory="$(ConfigurationName)"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="$(MAYAPATH)\include,$(TTLLIB)\Tools,$(TTLLIB)\GameLib"
				PreprocessorDefinitions="_WIN32;_DEBUG;_WINDOWS;NT_PLUGIN;WIN32_TOOLS_HYBRID;$(AVUSER)"
				BasicRuntimeChecks="3"
				RuntimeLibrary="2"
				UsePrecompiledHeader="3"
				PrecompiledHeaderThrough="pluginMainPCH.h"
				BrowseInformation="1"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Copying $(TargetFileName) to Maya Plugins Directory"
				CommandLine="copy &quot;$(TargetPath)&quot; &quot;$(MAYAPATH)\bin\plug-ins\$(TargetFileName)&quot;
"
				Outputs="$(MAYAPATH)\bin\plug-ins\$(TargetFileName)"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/export:initializePlugin /export:uninitializePlugin "
				AdditionalDependencies="openmayaui.lib foundation.lib openmaya.lib odbc32.lib odbccp32.lib"
				OutputFile="$(OutDir)/$(ProjectName).mll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				AdditionalLibraryDirectories="$(MAYAPATH)\lib"
				GenerateDebugInformation="TRUE"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG;$(AVUSER)"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\Maya_Debug/objAttrib.tlb"
				HeaderFileName=""/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG;$(AVUSER)"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCXMLDataGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
			<Tool
				Name="VCManagedWrapperGeneratorTool"/>
			<Tool
				Name="VCAuxiliaryManagedWrapperGeneratorTool"/>
		</Configuration>
	</Configurations>
	<References>
	</References>
	<Files>
		<Filter
			Name="Main Files"
			Filter="">
			<File
				RelativePath=".\ObjAttribCmd.cpp">
			</File>
			<File
				RelativePath=".\ObjAttribCmd.h">
			</File>
			<File
				RelativePath="pluginMain.cpp">
				<FileConfiguration
					Name="Release|Win32">
					<Tool
						Name="VCCLCompilerTool"
						Optimization="2"
						AdditionalIncludeDirectories=""/>
				</FileConfiguration>
				<FileConfiguration
					Name="Debug|Win32">
					<Tool
						Name="VCCLCompilerTool"
						Optimization="0"
						AdditionalIncludeDirectories=""
						BasicRuntimeChecks="3"
						BrowseInformation="1"/>
				</FileConfiguration>
			</File>
			<File
				RelativePath="pluginMain.h">
			</File>
			<File
				RelativePath="pluginMainPCH.cpp">
				<FileConfiguration
					Name="Release|Win32">
					<Tool
						Name="VCCLCompilerTool"
						Optimization="2"
						AdditionalIncludeDirectories=""
						UsePrecompiledHeader="1"/>
				</FileConfiguration>
				<FileConfiguration
					Name="Debug|Win32">
					<Tool
						Name="VCCLCompilerTool"
						Optimization="0"
						AdditionalIncludeDirectories=""
						BasicRuntimeChecks="3"
						UsePrecompiledHeader="1"
						BrowseInformation="1"/>
				</FileConfiguration>
			</File>
			<File
				RelativePath="pluginMainPCH.h">
			</File>
		</Filter>
		<Filter
			Name="Support MEL Scripts"
			Filter="">
			<File
				RelativePath="objAttrib.mel">
				<FileConfiguration
					Name="Release|Win32"
					ExcludedFromBuild="TRUE">
					<Tool
						Name="VCCustomBuildTool"/>
				</FileConfiguration>
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
